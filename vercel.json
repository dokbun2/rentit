{
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist/client",
  "public": true,
  "cleanUrls": true,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(assets|images|icons|fonts)/(.*)", "dest": "/$1/$2" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 