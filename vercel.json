{
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist/client",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "^/assets/(.*)", "dest": "/assets/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 